public class FindOdd {
    public static int findIt(int[] a) {
        int result = 0;
        
        for (int num : a) {
            result ^= num;
        }
        
        return result;
    }
}

class Kata {
    public static String numberToString(int num) {
        // Return a string of the number here!
        return String.valueOf(num);
    }
}
